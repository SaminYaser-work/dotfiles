{
  "coc.preferences.formatOnSaveFiletypes": ["css", "markdown", "js"],
  "prettier.useTabs": false,
  "prettier.tabWidth": 4,

// explorer
  "explorer.width": 30,
  "explorer.icon.enableNerdfont": true,
  "explorer.previewAction.onHover": "labeling",
  "explorer.keyMappings.global": {
    "<cr>": ["expandable?", "expand", "open"],
    "v": "open:vsplit"
  }

}
